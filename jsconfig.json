{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "baseUrl": "./",
    "moduleResolution": "node",
    "paths": {
      "@/*": [
        "src/*",
      ],
      "@core/*": [
        "src/engine/core/*"
      ],
      "@engine/*": [
        "src/engine/*"
      ],
      "@config/*": [
        "src/config/*"
      ],
    },
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ]
  }
}